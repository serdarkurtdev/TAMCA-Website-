{
  "name": "tamcaazure",
  "version": "1.0.0",
  "description": "initial release for new tamca dynamic azure website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "echo \"Building your web app...\" && npm run lint && npm run build-css && npm run build-js && npm run build-html",
